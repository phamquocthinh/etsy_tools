{
  "name": "etsy_tools",
  "main": "dist/server.js",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon src/server.js --watch src --exec babel-node",
    "build": "rimraf dist/ & babel src --out-dir dist --source-maps inline --copy-files",
    "lint": "esw *.js src --color",
    "lint:watch": "yarn lint -- --watch"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-3"
    ],
    "plugins": [
      "transform-runtime",
      "transform-async-generator-functions",
      "transform-object-rest-spread"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/"
  },
  "author": "Dev <github.com/>",
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.5.1",
    "body-parser": "~1.18.2",
    "cheerio": "^1.0.0-rc.3",
    "cookie-parser": "~1.4.3",
    "debug": "^3.1.0",
    "dotenv": "^5.0.1",
    "ejs": "^2.6.1",
    "express": "~4.16.3",
    "express-session": "^1.15.6",
    "jimp": "^0.6.1",
    "lodash": "^4.17.11",
    "moment": "^2.22.1",
    "mongoose": "^5.0.17",
    "mongoose-timestamp": "^0.6.0",
    "morgan": "~1.9.0",
    "multer": "^1.4.1",
    "oauth": "^0.9.15",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "serve-favicon": "^2.5.0",
    "sharp": "^0.22.0",
    "zip-a-folder": "^0.0.7"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-plugin-transform-async-generator-functions": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "eslint": "^4.19.1",
    "eslint-watch": "^3.1.4",
    "nodemon": "^1.17.4",
    "rimraf": "^2.6.2"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "rules": {
      "strict": 0
    }
  }
}
